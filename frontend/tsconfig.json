{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"allowJs": true,
		"checkJs": true,
		"isolatedModules": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noEmit": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
		  "@/*": ["src/*"]
		}
	  },
	  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.svelte"],
	  "references": [{ "path": "./tsconfig.node.json" }]
}